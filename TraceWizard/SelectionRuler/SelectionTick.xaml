<UserControl x:Class="TraceWizard.TwApp.SelectionTick"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:properties="clr-namespace:TraceWizard.TwApp.Properties"
>
    <Polygon Name="Tick" Points="0,0,1,1,0,2,-1,1,0,0" Height="6" Width="6" Margin="0,1" Stretch="Fill" StrokeThickness="1"
        Fill="{Binding Source={StaticResource Settings}, Path=Default.SelectionTickColor, Mode=OneWay}"                 
        Stroke="{Binding Source={StaticResource Settings}, Path=Default.SelectionTickColor, Mode=OneWay}" />
</UserControl>
