<UserControl x:Class="TraceWizard.TwApp.ApprovalTick"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:properties="clr-namespace:TraceWizard.TwApp.Properties"
>
    <Path Name="Tick" Data="M0,3 L3,6 L6,0" Height="6" Width="6" Stretch="Fill" Margin="0,1" StrokeThickness="1"
        Stroke="{Binding Source={StaticResource Settings}, Path=Default.SelectionTickColor, Mode=OneWay}" />
</UserControl>
